<Application x:Class="ToDue2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:ToDue2"
			 xmlns:converters="clr-namespace:ToDue2.Converters"
             StartupUri="MainWindow.xaml">
	<Application.Resources>
		<SolidColorBrush x:Key="Alert" Color="#FFFFAEBB"/>
		<SolidColorBrush x:Key="RedAlert" Color="#FFFF798E"/>
		<SolidColorBrush x:Key="Warning" Color="#FFECF38D"/>
		<SolidColorBrush x:Key="OK" Color="#FF83E8A7"/>
		<SolidColorBrush x:Key="LightForeground" Color="White"/>
	</Application.Resources>
</Application>
